{
  "name": "full-stack-web-application",
  "version": "1.0.0",
  "description": "Full stack web application for Tedu",
  "main": "index.js",
  "scripts": {
    "db:migration": "./node_modules/.bin/ts-node migrations/index.ts up",
    "server:dev": "nodemon --config nodemon.json app.ts",
    "start": "npm run db:migration && tsc && node --unhandled-rejections=strict ./dist/app.js",
    "debug": "export DEBUG=* && npm run start",
    "test": "ENV_FILE=./.env.test && mocha -r ts-node/register 'tests/**/*.test.ts' --timeout 20000 --unhandled-rejections=strict --exit",
    "build": "rimraf ./dist && tsc"
  },
  "keywords": [],
  "author": "Kiet Pham",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.30.3",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "express-winston": "^4.2.0",
    "joi": "^17.7.0",
    "mongo-migrate-ts": "^1.1.1",
    "mongoose": "^6.8.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.19",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.12",
    "@types/joi": "^17.2.3",
    "@types/mocha": "^8.2.2",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}